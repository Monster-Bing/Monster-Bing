class Solution(object):
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        n = len(nums) #在python中用len求列表元素
        i = 0
        while i < n - 1:
            if nums[i] == val:
                del nums[i] # 使用del函数删除
            i += 1
